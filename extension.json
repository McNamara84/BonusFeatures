{
	"name": "BonusFeatures",
	"version": "0.1.0",
	"author": [
		"Holger Ehrmann"
	],
	"url": "https://www.mediawiki.org/wiki/Extension:BonusFeatures",
	"descriptionmsg": "bonusfeatures-desc",
	"license-name": "MIT",
	"type": "other",
	"requires": {
		"MediaWiki": ">= 1.39.0"
	},
	"AutoloadNamespaces": {
		"MediaWiki\\Extension\\BonusFeatures\\": "src/"
	},
	"SpecialPages": {
		"BonusFeatures": {
			"class": "MediaWiki\\Extension\\BonusFeatures\\Special\\SpecialBonusFeatures"
		},
		"BonusHoerbuch": {
			"class": "MediaWiki\\Extension\\BonusFeatures\\Special\\SpecialBonusHoerbuch"
		},
		"BonusSchauplatzStatistiken": {
			"class": "MediaWiki\\Extension\\BonusFeatures\\Special\\SpecialBonusSchauplatzStatistiken"
		}
	},
	"MessagesDirs": {
		"BonusFeatures": [
			"i18n"
		]
	},
	"ExtensionMessagesFiles": {
		"BonusFeaturesAlias": "BonusFeatures.alias.php"
	},
	"ResourceModules": {
		"ext.bonusFeatures.init": {
			"scripts": [
				"resources/ext.bonusFeatures.init.js"
			],
			"dependencies": [
				"mediawiki.api",
				"mediawiki.util"
			]
		},
		"ext.bonusFeatures": {
			"styles": [
				"resources/ext.bonusFeatures.css"
			],
			"scripts": [
				"resources/ext.bonusFeatures.js"
			],
			"messages": [
				"bonusfeatures-specialpage-title"
			],
			"dependencies": []
		},
		"ext.bonusFeatures.statistics": {
			"styles": [
				"resources/ext.bonusFeatures.statistics.css",
				"https://cdn.datatables.net/2.1.2/css/dataTables.dataTables.min.css"
			],
			"scripts": [
				"resources/ext.bonusFeatures.statistics.js"
			],
			"dependencies": [
				"jquery",
				"mediawiki.api",
				"ext.bonusFeatures.externalLibraries"
			]
		},
		"ext.bonusFeatures.externalLibraries": {
			"scripts": [
				"https://cdn.datatables.net/2.1.2/js/dataTables.min.js",
				"https://cdn.jsdelivr.net/npm/chart.js",
				"https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.30.1/moment.min.js",
				"https://cdnjs.cloudflare.com/ajax/libs/chartjs-adapter-moment/1.0.1/chartjs-adapter-moment.min.js"
			],
			"targets": [
				"desktop",
				"mobile"
			]
		},
		"ext.bonusFeatures.data": {
			"class": "ResourceLoaderFileModule",
			"scripts": [],
			"styles": [],
			"dependencies": [],
			"messages": [],
			"targets": [
				"desktop",
				"mobile"
			],
			"defaultSkin": null,
			"skipFunction": null,
			"fileList": [
				"resources/maddrax.json"
			]
		}
	},
	"ResourceFileModulePaths": {
		"localBasePath": "",
		"remoteExtPath": "BonusFeatures"
	},
	"config": {
		"BonusFeaturesEnableXYZ": {
			"value": true
		}
	},
	"Hooks": {
		"BeforePageDisplay": "MediaWiki\\Extension\\BonusFeatures\\Hooks::onBeforePageDisplay"
	},
	"manifest_version": 2,
	"load_composer_autoloader": true,
	"platform": {
		"php": ">= 8.2.0"
	}
}